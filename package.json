{
  "name": "@finos/git-proxy",
  "version": "2.0.0-rc.2",
  "description": "Deploy custom push protections and policies on top of Git.",
  "scripts": {
    "cli": "node ./packages/git-proxy-cli/index.js",
    "client": "vite --config vite.config.ts",
    "clientinstall": "npm install --prefix client",
    "server": "tsx index.ts",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run build-ui && npm run build-lib",
    "build-ts": "tsc",
    "build-ui": "vite build",
    "build-lib": "./scripts/build-for-publish.sh",
    "restore-lib": "./scripts/undo-build.sh",
    "check-types": "tsc",
    "check-types:server": "tsc --project tsconfig.publish.json --noEmit",
    "test": "NODE_ENV=test ts-mocha './test/**/*.test.js' --exit",
    "test-coverage": "nyc npm run test",
    "test-coverage-ci": "nyc --reporter=lcovonly --reporter=text npm run test",
    "prepare": "node ./scripts/prepare.js",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" \"test/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,json}\" \"test/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} test/**/*.{js,jsx,ts,tsx,json} packages/git-proxy-cli/test/**/*.{js,jsx,ts,tsx,json}  packages/git-proxy-cli/index.js --config ./.prettierrc",
    "gen-schema-doc": "node ./scripts/doc-schema.js",
    "cypress:run": "cypress run"
  },
  "bin": {
    "git-proxy": "./index.js",
    "git-proxy-all": "concurrently 'npm run server' 'npm run client'"
  },
  "workspaces": [
    "./packages/git-proxy-cli"
  ],
  "author": "Paul Groves",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/finos/git-proxy"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "4.11.3",
    "@primer/octicons-react": "^19.15.5",
    "@seald-io/nedb": "^4.1.2",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "bit-mask": "^1.0.2",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "diff2html": "^3.4.52",
    "env-paths": "^2.2.1",
    "express": "^4.21.2",
    "express-http-proxy": "^2.1.1",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "history": "5.3.0",
    "isomorphic-git": "^1.32.2",
    "jsonschema": "^1.5.0",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "load-plugin": "^6.0.3",
    "lodash": "^4.17.21",
    "lusca": "^1.7.0",
    "moment": "^2.30.1",
    "mongodb": "^5.9.2",
    "nodemailer": "^6.10.1",
    "openid-client": "^6.6.2",
    "parse-diff": "^0.11.1",
    "passport": "^0.7.0",
    "passport-activedirectory": "^1.4.0",
    "passport-local": "^1.0.0",
    "perfect-scrollbar": "^1.5.6",
    "prop-types": "15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-html-parser": "^2.0.2",
    "react-router-dom": "6.30.1",
    "simple-git": "^3.28.0",
    "uuid": "^11.1.0",
    "validator": "^13.15.15",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/domutils": "^1.7.8",
    "@types/express": "^5.0.3",
    "@types/express-http-proxy": "^1.6.7",
    "@types/lodash": "^4.17.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.17.0",
    "@types/react-dom": "^17.0.26",
    "@types/react-html-parser": "^2.0.7",
    "@types/validator": "^13.15.2",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "chai": "^4.5.0",
    "chai-http": "^4.4.0",
    "cypress": "^14.5.3",
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "fast-check": "^4.2.0",
    "husky": "^9.1.7",
    "mocha": "^10.8.2",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "proxyquire": "^2.1.3",
    "sinon": "^21.0.0",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^4.5.14",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "^0.25.8",
    "@esbuild/darwin-x64": "^0.25.8",
    "@esbuild/linux-x64": "0.25.8",
    "@esbuild/win32-x64": "0.25.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
