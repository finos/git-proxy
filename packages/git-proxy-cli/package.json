{
  "name": "@finos/git-proxy-cli",
  "version": "2.0.0-rc.3",
  "description": "Command line interface tool for FINOS GitProxy.",
  "bin": {
    "git-proxy-cli": "./dist/index.js"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "yargs": "^17.7.2",
    "@finos/git-proxy": "file:../.."
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "ts-mocha": "^11.1.0"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"./*.ts\" --fix",
    "test:dev": "NODE_ENV=test ts-mocha test/*.ts --exit --timeout 10000",
